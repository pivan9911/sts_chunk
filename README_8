mvn clean compile -Pdb

# 2.1) stop
cd C:\UBS\Dev\svn\KDU8\KDU_CassrBuild\KDU_CassrDb
mvn antrun:run -Pstop-h2


# 3.) Oracle rebuild.
cd C:\UBS\Dev\svn\KDU8\KDU_CassrBuild\KDU_CassrDb
mvn clean compile -Pdb

