select * from TEST_RESULT where TEST_RESULT_ID = '91'

select * from TEST_RESULT where COMPONENT_ID = 'AA12244' and CONTROL_ID = '3' 
"select tr from TestResult tr where tr.componentId = ?1 and tr.controlType = ?2")

select COMMENT from TEST_RESULT where TEST_RESULT_ID = '90'

update TEST_RESULT set CONTROL_ID =  '5'   where   TEST_RESULT_ID = '500000'


select * from CONTROL where CONTROL_ID = '5'

select * from BAS_APPLICATION_DATA  where SWC_ID = 'AA12244' and 

select * from COMPONENT_ONBOARDING

select * BAS_CONTROL_ONBOARDING_STATUS
select * from  BAS_CONTROL_ONBOARDING_STATUS where SWC_ID='AA12244' and CTRL_ID='1'
http://localhost:8080/KDU_CassrWeb/updateControlOnboardingStatus.htm?swcId=AA12244&controlType=1

SELECT * FROM  TEST_RESULT TR,  CONTROL C   WHERE
           TR.COMPONENT_ID = 'AA12244'
AND          TR.CONTROL_ID = C.CONTROL_ID 
AND    C.CONTROL_METHOD_ID = 'VM1' 

SELECT * FROM  TEST_RESULT TR,  CONTROL C   WHERE
AND          TR.CONTROL_ID = C.CONTROL_ID 
AND    C.CONTROL_METHOD_ID = 'VM1' 


SELECT * FROM  TEST_RESULT TR
WHERE
           TR.COMPONENT_ID = 'AA12244'
AND          TR.CONTROL_ID = '1'
AND           ORF_RATING_DATE <  '2015-05-10'
ORDER BY 
           TR.ORF_RATING_DATE DESC;


SELECT * FROM  ASRC_STATE AT,  TEST_RESULT TR,  CONTROL C   WHERE
            AT.ASRC_STATE_ID =  TR.ASRC_STATE_ID 
AND           TR.CONTROL_ID = C.CONTROL_ID 
AND     C.CONTROL_METHOD_ID = 'VM1' 


SELECT  ORIG_ORF_RATING_ID,  CONF_ORF_RATING_ID FROM TEST_RESULT  WHERE TEST_RESULT_ID= '94'

